---
# Page meta info, like heading, footer text and nav links
pageInfo:
  title: Dashboard
  description: {{ portainer_dashy.description }}

# Optional app settings and configuration
appConfig:
  theme: lissy # dashy-docs, one-dark, lissy
  defaultOpeningMethod: sametab
  hideComponents:
    hideSettings: true
    hideFooter: true
    hideHeading: true
    hideSearch: true
    hideNav: true
  preventWriteToDisk: true
  statusCheckInterval: 20
  disableConfiguration: true
  webSearch:
    searchEngine: google
    openingMethod: sametab
  customCss: >
    .context-menu{
      display:none!important
    }
    html[data-theme=lissy] .home {
      padding-bottom:0!important
    }
    .item:focus {
      outline:none!important
    }
    .show-hide-container[data-v-0dc5c42a]{
      display:none
    }
    .edit-mode-item{
      display:none
    }
    .opening-method-icon svg{
      display:none
    }
    .tooltip{
      display:none!important
    }
    .indicator[data-v-4b522b5b]{
      cursor:pointer!important
    }
    .item.size-medium .tile-title {
      height:auto
    }
    .item-icon.wrapper-medium {
      display:flex;
      align-items:center;
      padding:5px 0
    }
    .item.size-medium div img {
      margin-bottom:0
    }
    .tile-icon {
      object-fit: contain!important;
    }

